<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>

	<class name="com.k9sv.domain.pojo.Feedback" table="feedback">
		<id name="id" type="int">
			<column name="id" />
			<generator class="native" />
		</id>
		<property name="uid" column="uid" type="int" length="11" />
		<property name="relationId" column="relation_id" type="int"
			length="11" />
		<property name="type" column="type" type="int" length="11" />
		<property name="content" column="content" type="java.lang.String" /><!-- 
			内容 -->
		<property name="created" column="created" type="java.lang.String" /><!-- 
			发布时间 -->
	</class>
</hibernate-mapping>
